{
  "name": "leapp",
  "description": "Leapp's Command Line Interface",
  "version": "0.1.0",
  "author": "besharp <info@besharp.it>",
  "bin": {
    "leapp": "./bin/run"
  },
  "bugs": "https://github.com/Noovolari/leapp/issues",
  "dependencies": {
    "@noovolari/leapp-core": "file:../core/dist",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-legacy": "^1.2.7",
    "@oclif/plugin-plugins": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/crypto-js": "^4.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2.0.0-main.10",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/noovolari/leapp",
  "keywords": [
    "oclif"
  ],
  "main": "dist/index.js",
  "oclif": {
    "bin": "leapp",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "start": {
        "description": "Start Session"
      }
    }
  },
  "repository": "noovolari/leapp",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
